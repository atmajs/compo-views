UTest({
	$before (next) {
		UTest
			.server
			.request('http://localhost:5777/examples/tabs/async.html')
			.done((doc, win) => {
				next(doc, win);
			});
	},
	'should load and switch tabs async' (done, doc, win) {
		UTest
			.domtest(doc.body, `
				hasNot ('b');
				await (300);
				has ('b');

				find ('.navigate > [data-route="?two"]') {
					do click;
				}
				hasNot('em');
				has('.-a-views-progress');
				has('b');
				await('em');
				hasNot('b');
			`)
			.always(() => done(doc, win));
	},

})