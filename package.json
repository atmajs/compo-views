{
  "name": "compo-views",
  "version": "0.10.43",
  "devDependencies": {
    "atma": ">0.9.26",
    "atma-loader-babel": "^1.1.30",
    "atma-server": "^0.1.34",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "test": "atma test",
    "examples": "node examples/node/index",
    "build": "atma"
  },
  "atma": {
    "plugins": [
      "atma-loader-babel"
    ],
    "settings": {
      "atma-loader-babel": {
        "extensions": [
          "es6",
          "test"
        ],
        "babel": {
          "sourceMap": true,
          "presets": [
            [
              "es2015"
            ]
          ]
        }
      }
    }
  },
  "dependencies": {
    "maskjs": "^0.54.26",
    "ruta": "^0.2.20",
    "static-content": "^1.1.23"
  }
}
